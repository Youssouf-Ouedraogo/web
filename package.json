{
  "name": "web",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "axios": "^0.19.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-scripts": "3.3.0"
  },
  "devDependencies": {
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint-config-airbnb": "set_to_the_version_listed_in_step_2",
    "eslint-plugin-import": "set_to_the_version_listed_in_step_2",
    "eslint-plugin-jsx-a11y": "set_to_the_version_listed_in_step_2",
    "eslint-plugin-react": "set_to_the_version_listed_in_step_2",
    "eslint-plugin-react-hooks": "set_to_the_version_listed_in_step_2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:cover": "react-scripts test --coverage",
    "lint": "eslint 'src/**/*.js'",
    "posttest": "yarn lint"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest":{
    "collectCoverageFrom": ["src/**/*.{js,jsx,ts,tsx}"],
    "coverageThreshold": {
      "global":{
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    }
  }
}
